<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   <bean id="smart" class="com.deloitte.Smartphone">
     <property name="name" value="Asus"></property>
 
   <property name="price" value="1350.23"></property>
   <constructor-arg ref="app1"></constructor-arg>
   </bean>
   <bean id="app1" class="com.deloitte.Application">
     <property name="name" value="candycrush"></property>
   <property name="type" value="game"></property>
   </bean>
   </beans>