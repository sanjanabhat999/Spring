<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id="CollectionExample" class="com.deloitte.CollectionExample">
   
   <property name="cityList">
   <list>
   <value>Bangalore</value>
   <value>Mangalore</value>
   <value>Udupi</value>
   <value>Pune</value>
   </list>
   </property>
   
   <property name="cityMap">
   <map>
   <entry key="1" value="Bangalore"></entry>
    <entry key="2" value="Mangalore"></entry>
     <entry key="3" value="Udupi"></entry>
      <entry key="4" value="Pune"></entry>
   </map>
   </property>
   
   <property name="citySet">
   <set>
   <value>Bangalore</value>
   <value>Mangalore</value>
   <value>Udupi</value>
   <value>Pune</value>
   </set>
   </property>
   
   
   <property name="cityProp">
   <props>
   <prop key="one" >Bangalore</prop>
    <prop key="two"> Mangalore</prop>
     <prop key="three"> Udupi</prop>
      <prop key="four"> Pune</prop>
   </props>
   </property>
   
   
   </bean>
   </beans>
  